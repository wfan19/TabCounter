{
    "name": "Tab Counter",
    "version": "0.1",
    "description": "An easy way to count tabs for tab-fiends like you and me. ",
  "permissions": 
  [
    "tabs"
  ],
	"background": {
      "scripts": 
      [
        "background.js",
        "jquery-3.3.1.js"
      ],
      "persistent": false
    },
  "browser_action": 
  {
    "default_popup": "popup.html",
    "default_icon": 
    {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
      }
    },
  "icons": 
    {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "manifest_version": 2
}